for (var n = 1; n <= 10; n = n + 1) {
  var start = clock();
  
  // Calculate n!
  var factorial = 1;
  for (var i = 1; i <= n; i = i + 1) {
    factorial = factorial * i;
  }
  
  // Do 'factorial' amount of work
  var work = 0;
  for (var j = 1; j <= factorial; j = j + 1) {
    work = work + j * j;
  }
  
  var time = clock() - start;
  print n;
  print time; 
}
